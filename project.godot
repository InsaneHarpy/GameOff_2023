[autoload]

Global="*res://global.gd"
BackgroundMusic="*res://sounds/background_music.tscn"

[display]

window/size/viewport_width=900
window/size/viewport_height=750
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Papa's Adoptoria"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[gui]

theme/custom_font="res://art-assets/fonts/MaintTextFont.ttf"

[input]

